#!/bin/sh
# Used for general, on-startup actions.

# Computer-specific settings
source ~/.ss

# Change background.
feh --bg-scale ~/dotfiles/background.jpg

# run urxvt daemon
urxvtd&

# emacs setup
export ALTERNATE_EDITOR=emacs EDITOR=emacsclient VISUAL=emacsclient
#emacs --daemon&

# Add cabal binaries
export PATH=/home/aapollon/.cabal/bin/:$PATH


